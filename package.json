{
  "name": "use-my-tech-stuff-back-end",
  "version": "1.0.0",
  "description": "back end for use-my-tech-stuff app",
  "scripts": {
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "mup":"knex migrate:up",
    "mdown":"knex migrate:down",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback && npm run migrate && npm run seed",
    "test": "cross-env NODE_ENV=testing jest --watch --verbose --runInBand",
    "migrateh": "heroku run knex migrate:latest -a usemytechstuff-bw",
    "rollbackh": "heroku run knex migrate:rollback -a usemytechstuff-bw",
    "databaseh": "heroku pg:psql -a usemytechstuff-bw",
    "seedh": "heroku run knex seed:run -a usemytechstuff-bw",    
    "deploy": "git push heroku main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptct-use-my-tech-3/use-my-tech-stuff-back-end.git"
  },
  "author": "Stephen Aranda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ptct-use-my-tech-3/use-my-tech-stuff-back-end/issues"
  },
  "homepage": "https://github.com/ptct-use-my-tech-3/use-my-tech-stuff-back-end#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.7",
    "knex-cleaner": "^1.3.1",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "cross-env": "^7.0.3",
    "jest": "^27.0.6",
    "jwt-decode": "^3.1.2",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.3"
  }
}
